pipeline {
    agent any
    stages {
        stage('### cleanup ###') {
            steps {
                deleteDir()
            }
        }
        stage('### clone project ###') {
            steps {
                bat 'git clone '
            }
        }
        stage('### build ###') {
            steps {
                dir('jenkinstest') {
                    bat 'mvn clean install'
                }
            }
        }
        stage('### Final ###') {
            steps {
                bat 'echo finish'
            }
        }
    }
}